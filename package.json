{
  "name": "webpack-html-generator-plugin",
  "version": "2.1.0",
  "description": "A webpack plugin to generate html entry for each js entry",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --recursive ./src/**/*.spec.ts",
    "lint": "tslint ./src/**/*.ts",
    "build": "yarn run lint && rm -rf ./lib && tsc",
    "prepublish": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acrazing/webpack-html-generator-plugin.git"
  },
  "keywords": [
    "webpack",
    "html",
    "generate",
    "generator",
    "plugin"
  ],
  "author": "acrazing <joking.young@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acrazing/webpack-html-generator-plugin/issues"
  },
  "homepage": "https://github.com/acrazing/webpack-html-generator-plugin#readme",
  "dependencies": {
    "@types/clean-css": "^3.4.30",
    "@types/ejs": "^2.3.33",
    "@types/html-minifier": "^3.5.2",
    "@types/node": "^10.5.4",
    "@types/relateurl": "^0.2.28",
    "@types/uglify-js": "^3.0.3",
    "ejs": "^2.5.7",
    "html-minifier": "^3.5.5",
    "monofile-utilities": "^1.9.1",
    "tslib": "^1.9.3"
  },
  "devDependencies": {
    "@types/webpack": "^4.4.8",
    "autoprefixer": "^9.0.2",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.1",
    "normalize.css": "^8.0.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.6",
    "ts-loader": "^4.4.2",
    "typescript": "^3.0.1",
    "webpack": "^4.16.3"
  }
}
