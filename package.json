{
  "name": "webpack-html-generator-plugin",
  "version": "0.1.0",
  "description": "A webpack plugin to generate html entry for each js entry",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --compilers ts:ts-node/register,tsx:ts-node/register --recursive ./src/**/*.spec.ts",
    "lint": "tslint ./src/**/*.ts",
    "build": "yarn run lint && rm -rf ./lib && tsc",
    "prepublish": "yarn run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acrazing/webpack-html-generator-plugin.git"
  },
  "keywords": [
    "webpack",
    "html",
    "generate",
    "generator",
    "plugin"
  ],
  "author": "acrazing <joking.young@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/acrazing/webpack-html-generator-plugin/issues"
  },
  "homepage": "https://github.com/acrazing/webpack-html-generator-plugin#readme",
  "dependencies": {
    "@types/clean-css": "^3.4.30",
    "@types/ejs": "^2.3.33",
    "@types/html-minifier": "^1.1.30",
    "@types/node": "^6.0.52",
    "@types/relateurl": "^0.2.28",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.0.0",
    "ejs": "^2.5.5",
    "html-minifier": "^3.2.3",
    "path-rewrite-plugin": "^0.0.5"
  },
  "devDependencies": {
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ts-loader": "^1.3.3",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0"
  }
}
